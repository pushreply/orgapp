<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    

	<TextView
	    android:id="@+id/PREVIOUS_EVENT_NAME"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:textSize="@dimen/PROFIL_PREVIOUS_EVENT_NAME_SIZE" />

	<TextView
	    android:id="@+id/PREVIOUS_EVENT_DATE"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:textSize="@dimen/PROFIL_PREVIOUS_EVENT_DATE_SIZE" />
        
</LinearLayout>